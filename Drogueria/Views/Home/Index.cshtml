

@*@{
    ViewBag.Title = "Index";
    Layout = "~/Views/LandingNew/LandingNew.cshtml";
}*@


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="~/images/Hall9000_edo.png" />
    <title>test</title>
    <!--Custom styles-->
    <link rel="stylesheet" type="text/css" href="~/RecursosExternos/semantic.css">
    <link rel="stylesheet" type="text/css" href="~/RecursosExternos/semantic.min.css">


    <link href="~/RecursosExternos/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="~/RecursosExternos/bootstrap.min.js"></script>
    <script src="~/RecursosExternos/jquery.min.js"></script>

    <!-- Semantic UI CSS -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.1.8/semantic.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.css">
    <script type="text/javascript" language="javascript" src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.js"></script>

    <style>


        html {
            background: url('/Images/Wall1.jpg') no-repeat center center fixed;
            background-size: cover;
        }

        body {
            width: 100%;
            height: 100%;
            background-image: url('/Images/Wall1.jpg');
            background-position: center;
            background-size: 100% 100%;
            background-repeat: no-repeat;
        }
    </style>
</head>
<body>
    <br />
    <br />
    <br />


    <div class="ui link cards">
        <div class="container">
            <div style="height:5px"></div>
            <h2 style="color:white" class="ui center aligned header">
                @*<img src="~/Recursos/Imagenes/IconoAsmar.png" class="ui image">*@
                Bienvenido @SessionH.Usuario.Nombre
            </h2>
            <div class="ui link cards">
                <div class="container">                    
                    <div class="row">
                        @*MantenedorMaquinarias*@
                        <div class="col-md-4">
                            <div class="card mb-4 shadow-sm" style="opacity:.85">
                                <div class="contenedor-imagen">
                                    <img class="card-img-top" src="~/Images/Schedule.png" alt="Card image cap">
                                </div>
                                <div class="card-body">
                                    <p class="card-text">Aquí podras reserver tu maquinaria.</p>
                                    <div class="d-flex justify-content-between align-items-center">
                                        <div class="btn-group">
                                            <div id="btnMarketPlace" onclick="window.location.href = '/Solicitudes'" class="ui right labeled icon black button">
                                                Agenda
                                                <i class="hand point left icon"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*MantenedorUsuarios*@
                        <div class="col-md-4">
                            <div class="card mb-4 shadow-sm" style="opacity:.85">
                                <div class="contenedor-imagen">
                                    <img class="card-img-top" src="~/Images/Solicitudes.png" alt="Card image cap">
                                </div>
                                <div class="card-body">
                                    <p class="card-text">Aquí podras asignar y descargar la programación.</p>
                                    <div class="d-flex justify-content-between align-items-center">
                                        <div class="btn-group">
                                            <div id="btnMarketPlace" onclick="window.location.href = '/ProgramacionSolicitud'" class="ui right labeled icon black button">
                                                Programación solicitud
                                                <i class="hand point left icon"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*MantenedorUsuarios*@
                        <div class="col-md-4">
                            <div class="card mb-4 shadow-sm" style="opacity:.85">
                                <div class="contenedor-imagen">
                                    <img class="card-img-top" src="~/Images/Solicitudes.png" alt="Card image cap">
                                </div>
                                <div class="card-body">
                                    <p class="card-text">Aquí podras asignar y descargar la programación.</p>
                                    <div class="d-flex justify-content-between align-items-center">
                                        <div class="btn-group">
                                            <div id="btnMarketPlace" onclick="window.location.href = '/ProgramacionSolicitud'" class="ui right labeled icon black button">
                                                Programación solicitud
                                                <i class="hand point left icon"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*Salir*@
                        <div class="col-md-4">
                            <div class="card mb-4 shadow-sm" style="opacity:.85">
                                <div class="contenedor-imagen">
                                    <img class="card-img-top" src="~/Images/Solicitudes.png" alt="Card image cap">
                                </div>
                                <div class="card-body">
                                    <p class="card-text">Aquí podras asignar y descargar la programación.</p>
                                    <div class="d-flex justify-content-between align-items-center">
                                        <div class="btn-group">
                                            <div id="btnMarketPlace" onclick="window.location.href = '/ProgramacionSolicitud'" class="ui right labeled icon black button">
                                                Programación solicitud
                                                <i class="hand point left icon"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



</body>
</html>









